PROGRAM nomedoprograma
BEGIN
    FUNCTION VOID main()
    BEGIN
        INT a , b , r , i;

        READ ( a , b );

        r := 0 ;

		IF ( a > b ) THEN
		    r := soma ( a , b );
		ELSE
			FOR ({i := 0}; {i < a}; {i := i + 1})
				r := r b + ;
			ENDFOR
		ENDIF

        WRITE ( r );

    END

    FUNCTION INT soma (INT x , INT y)
    BEGIN
        RETURN x y + ;
    END
END
